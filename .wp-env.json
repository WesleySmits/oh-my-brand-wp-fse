{
    "$schema": "https://schemas.wp.org/trunk/wp-env.json",
    "core": "WordPress/WordPress#6.7",
    "phpVersion": "8.4",
    "plugins": [],
    "themes": [
        "https://downloads.wordpress.org/theme/ollie.latest-stable.zip",
        "."
    ],
    "config": {
        "WP_DEBUG": true,
        "SCRIPT_DEBUG": true,
        "WP_DEBUG_LOG": true,
        "WP_DEBUG_DISPLAY": false
    },
    "mappings": {
        "wp-content/themes/oh-my-brand": "."
    },
    "lifecycleScripts": {
        "afterStart": "wp-env run cli bash wp-content/themes/oh-my-brand/tests/e2e/bin/setup-fixtures.sh"
    },
    "env": {
        "tests": {
            "phpVersion": "8.4",
            "config": {
                "WP_DEBUG": true,
                "SCRIPT_DEBUG": true
            }
        }
    }
}
